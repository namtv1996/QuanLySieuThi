        $('button#bt.btn-create.event-Free-Trial-form-submit').click(function (event) {
            if ($('input#FullName.form-control.form-input')[0].validity.valid==true && $('input#PhoneNo.form-control.form-input')[0].validity.valid==true && $('input#Email.form-control.form-input')[0].validity.valid==true && $('input#TenantName.form-control.form-input.margin-sm-bottom.tencuahang')[0].validity.valid==true && $('input#Username.form-control.form-input')[0].validity.valid==true && $('input#Password.form-control.form-input')[0].validity.valid==true && $('input#cbIsConfirm.big-checkbox')[0].validity.valid==true && $('select#IndustryId.form-control.form-input')[0].validity.valid==true && $('select#IndustryId.form-control.form-input')[1].validity.valid==true ) {
                $('.container').show();
                $('#check').show();
                var timeleft = 10;
                var downloadTimer = setInterval(function () {
                    document.getElementById("progressBar").value = 10 - --timeleft;
                    if (timeleft <= 0)
                        clearInterval(downloadTimer);
                    if (timeleft <= 0) {
                        $('#create').show();
                        $('#check').hide(1000);
                    }


                }, 1000);

            }

        });
